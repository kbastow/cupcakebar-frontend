adding: 1em;\n  background: #fff;\n}\n\n// root ---------------\n#root {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  overflow: hidden;\n}\n\n// page --------------------\n.page-content {\n  position: absolute;\n  top: var(--app-header-height);\n  left: 0;\n  right: 0;\n  bottom: 0;\n  overflow-y: scroll;\n  padding: 1em;\n  &.no-pad {\n    padding: 0;\n  }\n}\n\n.page-content-post {\n  position: absolute;\n  top: var(--app-header-height);\n  left: 0;\n  right: 0;\n  bottom: 0;\n  overflow-y: scroll;\n  padding: 1em;\n  &.no-pad {\n    padding: 0;\n  }\n}\n\n.bio {\n  display: flex;\n  border: 4px solid #fff;\n  background-color: rgba(0, 0, 0, 0.2);\n  border-radius: 15px;\n  width: 80%;\n  margin-left: auto;\n  margin-right: auto;\n  margin-bottom: 10px;\n  padding: 10px;\n  color: #fff;\n  align-items: right;\n}\n\n.page-centered {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.page-title {\n  font-family: var(--body-font-family);\n}\n\n.prof-border {\n  margin-top: 20px;\n  max-width: 800px;\n  padding: 20px;\n  margin-left: auto;\n  margin-right: auto;\n  text-align: center;\n  border: 4px solid #fff;\n  background-color: rgba(255, 255, 255, 0.3);\n  border-radius: 25px;\n  height: auto;\n}\n\n.prof-txt {\n  color: var(--brand-color);\n}\n\n.prof-txt-secondary {\n  color: #fff;\n}\n\n.form-box {\n  margin-top: 20px;\n  max-width: 500px;\n  padding: 20px;\n  margin-left: auto;\n  margin-right: auto;\n  text-align: center;\n  border: 4px solid var(--sl-color-primary-100);\n  background-color: rgba(0, 0, 0, 0.2);\n  border-radius: 25px;\n  height: auto;\n}\n\n// sign in + sign up pages ----------\n.signinup-box,\n.signup-box {\n  //max-width: 320px;\n  text-align: center;\n}\n\n.signinup-logo {\n  width: 20%;\n  margin-bottom: 1em;\n  margin-left: auto;\n  margin-right: auto;\n}\n\n.home-logo {\n  width: 50%;\n  margin-top: 1.5em;\n  height: auto;\n}\n\n.link {\n  text-decoration: none;\n  color: #fff;\n}\n\n.link:hover {\n  color: var(--brand-color);\n}\n\n// guide page ---------------\n.guide-step {\n  padding: 1em;\n  margin: 1.5em auto;\n  max-width: 600px;\n  img {\n    width: 100%;\n    height: auto;\n  }\n}\n\n.workers-grid {\n  display: inline-block;\n  align-items: center;\n  margin-left: 10px;\n  margin-right: 10px;\n  //flex-wrap: wrap;\n\n  .worker-card {\n    width: calc(33.3% - 1em);\n    margin: 0.5em;\n  }\n}\n\n.filter-by {\n  margin-right: 10px;\n}\n\n.jobs-grid {\n  /*display: flex;\n  flex-wrap: wrap;*/\n  display: inline-block;\n  align-items: center;\n  margin-left: 10px;\n  margin-right: 10%;\n\n  .job-card {\n    width: calc(33.3% - 1em);\n    margin: 0.5em;\n  }\n}\n\n.job-desc-title {\n  color: var(--heading-color);\n}\n\n.worker-dialog {\n  --width: 50%;\n}\n\n.worker-dialog::part(body) {\n  margin-top: -4em;\n}\n\n.worker-dialog::part(panel) {\n  background: var(--sl-color-primary-50);\n  border: 3px solid #fff;\n  border-radius: 15px;\n  color: white;\n}\n\n.worker-bio {\n  width: auto;\n}\n\n.dialog-worker-name {\n  color: #fff;\n}\n\n.job-dialog {\n  --width: 30%;\n}\n\n.job-dialog::part(body) {\n  margin-top: -4em;\n  color: #fff;\n}\n\n.job-dialog::part(close-button):hover {\n  color: var(--brand-color);\n}\n\n.job-dialog::part(panel) {\n  background: var(--sl-color-primary-50);\n  border: 3px solid #fff;\n  border-radius: 15px;\n}\n\n// RESPONSIVE - MOBILE -------------------\n@media all and (max-width: 768px) {\n  .signinup-box {\n    width: 90%;\n    max-width: none;\n  }\n}\n","scss/_forms.scss":"// Forms\n\n.input-group {\n  margin-bottom: 0.5em;\n}\n.page-form {\n  max-width: 700px;\n}\n","scss/_shoelace.scss":"// Shoelace\n\n// sl-dialog\nsl-dialog::part(title) {\n  font-weight: 500;\n  font-size: 1.6em;\n}\n\nsl-input::part(base) {\n  background: var(--sl-color-primary-50);\n  border-radius: 20px;\n  border: 3px solid var(--brand-color);\n  max-width: 35%;\n  position: center;\n  margin-bottom: 0.2em;\n  margin-left: auto;\n  margin-right: auto;\n  margin-top: 1em;\n}\n\nsl-select::part(suffix) {\n  background: var(--sl-color-primary-100);\n  border-radius: 15px;\n}\n\nsl-textarea::part(base) {\n  background: var(--sl-color-primary-100);\n  border-radius: 15px;\n  margin-bottom: 1em;\n}\n\nsl-button::part(base) {\n  border-radius: 20px;\n  width: 10em;\n  border: 3px solid var(--brand-color);\n  background: var(--sl-color-primary-50);\n  color: var(--brand-color);\n}\n\nsl-card {\n  --border-radius: 15em;\n}\n\n// RESPONSIVE - MOBILE -------------------\n@media all and (max-width: 768px) {\n  sl-dialog::part(panel) {\n    width: 100%;\n    height: 100%;\n    max-height: none;\n    max-width: none;\n  }\n}\n","scss/_toast.scss":"// Toast\n\n#toasts {\n  position: fixed;\n  left: 50%;  \n  top: 1em; \n  transform: translateX(-50%); \n  z-index: 999;\n  display: flex;\n  justify-content: center;\n  flex-direction: column; \n}\n\n.toast-entry {  \n  background:#fff;\n  margin: 0 auto;\n  color: #333;  \n  border-radius: 200px;\n  box-shadow: 0px 3px 25px rgba(0,0,0,0.14);\n  padding: .7em 1.3em;\n  font-size: 1em;\n  margin-bottom: .25em;\n  text-align: center;\n  transition: all .2 ease;\n  &.is-error {\n    background: rgb(212, 25, 93);\n    color:#fff;\n  }  \n}"},"lineCount":null}},"error":null,"hash":"c19a14719b5d21d98253551334c6a8ef","cacheData":{"env":{}}}